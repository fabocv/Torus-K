{
  "name": "torus-k",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "publish": "npx angular-cli-ghpages --dir=dist/torus-k/browser",
    "build": "ng build --configuration=production --base-href /Torus-K/ && cp dist/torus-k/browser/index.csr.html dist/torus-k/browser/index.html && cp dist/torus-k/browser/index.html dist/torus-k/browser/404.html",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:torus-k": "node dist/torus-k/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/platform-server": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@angular/ssr": "^21.1.0",
    "@fontsource/fira-code": "^5.2.7",
    "@tailwindcss/postcss": "^4.2.0",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
